/*
PRN 22070123085
Experiment - 9
Printing Hello World 10 times
*/

#include <iostream>
using namespace std;

int main()

{
    int i = 0;
    for(i = 1; i <=10; ++i)
    {
        cout<<"Hello World"<<endl;
    }
}

/*
OUTPUT: 
Hello World
Hello World
Hello World
Hello World
Hello World
Hello World
Hello World
Hello World
Hello World
Hello World
*/
