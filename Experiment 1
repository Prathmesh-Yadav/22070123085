*/


// CALCULATOR
#include <iostream>
using namespace std;
int main()
{
    int a,b;
    cout<<"Enter the value of a:"<<endl;
    cin>>a;

    cout<<"Enter the value of b:"<<endl;
    cin>>b;

    cout<<"The sum of the numbers is:"<<a+b<<endl;
    cout<<"The subtraction of the numbers is:"<<a-b<<endl;
    cout<<"The multiplication of the numbers is:"<<a*b<<endl;
    cout<<"The division of the numbers is:"<<a/b<<endl;

    return 0;
}

/*
OUTPUT-
Enter the value of a:
20
Enter the value of b:
10
The sum of the numbers is:30
The subtraction of the numbers is:10
The multiplication of the numbers is:200
The division of the numbers is:2
*/
